{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "workspace:create": "turbo gen workspace --copy"
  },
  "devDependencies": {
    "turbo": "^2.6.3"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=18"
  }
}
